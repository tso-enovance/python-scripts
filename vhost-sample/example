<VirtualHost *:80>
    ServerName www.exemple.com
    ServerAlias exemple.com 

    DocumentRoot /home/sites_web/exemple/www

    <Directory /home/sites_web/exemple/www>
        Order Deny,Allow
        Allow from all
        Options -Indexes MultiViews FollowSymLinks
        AllowOverride all
    </Directory>

    ErrorLog /home/logs/apache2/exemple/error-exemple.log
    CustomLog /home/logs/apache2/exemple/access-exemple.log combined
</VirtualHost>

